{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "simbad-client": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "changeDetection": "OnPush",
                    "style": "scss"
                }
            },
            "root": "projects/simbad-client",
            "sourceRoot": "projects/simbad-client/src",
            "prefix": "simbad",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/simbad-client",
                        "index": "projects/simbad-client/src/index.html",
                        "main": "projects/simbad-client/src/main.ts",
                        "polyfills": "projects/simbad-client/src/polyfills.ts",
                        "tsConfig": "projects/simbad-client/tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "projects/simbad-client/src/favicon.ico",
                            "projects/simbad-client/src/assets"
                        ],
                        "styles": [
                            "projects/simbad-client/src/styles-app-loading.scss",
                            "projects/simbad-client/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/simbad-client/src/environments/environment.ts",
                                    "with": "projects/simbad-client/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "bundle",
                                    "name": "polyfills",
                                    "baseline": "150kb",
                                    "maximumWarning": "50kb",
                                    "maximumError": "100kb"
                                },
                                {
                                    "type": "bundle",
                                    "name": "styles",
                                    "baseline": "280kb",
                                    "maximumWarning": "50kb",
                                    "maximumError": "100kb"
                                },
                                {
                                    "type": "bundle",
                                    "name": "main",
                                    "baseline": "1024kb",
                                    "maximumWarning": "100kb",
                                    "maximumError": "200kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "simbad-client:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "simbad-client:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "simbad-client:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/simbad-client/src/test.ts",
                        "polyfills": "projects/simbad-client/src/polyfills.ts",
                        "tsConfig": "projects/simbad-client/tsconfig.spec.json",
                        "karmaConfig": "projects/simbad-client/karma.conf.js",
                        "assets": [
                            "projects/simbad-client/src/favicon.ico",
                            "projects/simbad-client/src/assets"
                        ],
                        "styles": [
                            "projects/simbad-client/src/styles-app-loading.scss",
                            "projects/simbad-client/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "test": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/simbad-client/src/environments/environment.ts",
                                    "with": "projects/simbad-client/src/environments/environment.test.ts"
                                }
                            ]
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/simbad-client/tsconfig.app.json",
                            "projects/simbad-client/tsconfig.spec.json",
                            "projects/simbad-client/e2e/tsconfig.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/simbad-client/e2e/protractor.conf.js",
                        "devServerTarget": "simbad-client:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "simbad-client:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "simbad-client"
}
